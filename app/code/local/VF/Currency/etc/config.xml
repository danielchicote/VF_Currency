<config>
    <phpunit>
        <suite>
            <modules>
                <VF_Currency/>
            </modules>
        </suite>
    </phpunit>
    <global>
        <models>
            <directory>
                <rewrite>
                    <currency>VF_Currency_Model_Directory_Currency</currency>
                </rewrite>
            </directory>
            <core>
                <rewrite>
                    <locale>VF_Currency_Model_Core_Locale</locale>
                </rewrite>
            </core>
        </models>
    </global>
</config>

